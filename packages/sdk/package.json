{
  "name": "@keon/sdk",
  "version": "1.0.0",
  "description": "Keon Runtime SDK - TypeScript client for governance-first execution",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "echo 'Linting not configured yet'"
  },
  "keywords": [
    "keon",
    "sdk",
    "typescript",
    "governance",
    "runtime"
  ],
  "author": "Keon Systems",
  "license": "Apache-2.0",
  "dependencies": {
    "@keon/contracts": "workspace:*",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  }
}
